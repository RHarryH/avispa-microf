{
  "propertyPage": {
    "controls": [
      {
        "type": "text",
        "label": "Company name",
        "required": true,
        "property": "companyName"
      },
      {
        "type": "text",
        "label": "VAT Identification Number",
        "property": "vatIdentificationNumber",
        "required": true,
        "pattern": "[0-9]{3}-[0-9]{2}-[0-9]{2}-[0-9]{3}",
        "customValidation": {
          "function": "NIPIsValid",
          "message": "Vat Identification Number is invalid"
        }
      },
      {
        "type": "text",
        "label": "Phone number",
        "property": "phoneNumber",
        "pattern": "\\+48 \\d{9}"
      },
      {
        "type": "email",
        "label": "Email",
        "property": "email"
      },
      {
        "type": "label",
        "expression": "'Customer address'"
      },
      {
        "type": "text",
        "label": "Street",
        "property": "address.street",
        "required": true
      },
      {
        "type": "columns",
        "controls": [
          {
            "type": "text",
            "label": "Zip code",
            "property": "address.zipCode",
            "required": true,
            "pattern": "[0-9]{2}-[0-9]{3}"
          },
          {
            "type": "text",
            "label": "Place",
            "property": "address.place",
            "required": true
          }
        ]
      }
    ]
  }
}